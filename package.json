{
  "name": "femto-flux",
  "version": "2.1.0-0",
  "description": "minimal flux",
  "keywords": [
    "dispatcher",
    "flux",
    "minimal",
    "store"
  ],
  "homepage": "https://github.com/commenthol/femto-flux#readme",
  "bugs": {
    "url": "https://github.com/commenthol/femto-flux/issues"
