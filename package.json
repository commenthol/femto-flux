    "jsdoc": "^3.6.3",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "preact": "^10.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
