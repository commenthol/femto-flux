{
  "name": "femto-flux",
  "version": "2.1.1",
  "description": "minimal flux",
  "keywords": [
    "dispatcher",
    "flux",
    "minimal",
    "store"
  ],
  "homepage": "https://github.com/commenthol/femto-flux#readme",
  "bugs": {
    "url": "https://github.com/commenthol/femto-flux/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/femto-flux.git"
  },
  "license": "Unlicense",
  "author": "commenthol@gmail.com",
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es6.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean lint build test",
    "build": "rollup -c",
    "build:example": "cd example && webpack",
    "clean": "rimraf dist coverage .nyc_output",
    "clean:all": "npm-run-all clean clean:node_modules",
    "clean:modules": "rimraf node_modules",
    "coverage": "nyc -r text -r html npm test",
    "doc": "jsdoc -c scripts/jsdoc/jsdoc.json",
    "gh-pages": "npm run build:example && cp example/container.bundle.js . && cp example/container.html index.html",
    "lint": "eslint --ext .js .",
    "prepublishOnly": "npm run all",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha"
  },
  "mocha": {
    "require": [
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-external-helpers": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "babel-loader": "^8.2.5",
    "core-js": "^3.25.1",
    "eslint": "^8.23.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "jsdoc": "^3.6.11",
    "jsdom": "^20.0.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "preact": "^10.11.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
